{
  "name": "BlinkBoardApp",
  "version": "0.1.0",
  "description": "Using your breadboard for online coursework",
  "main": "main.js",
  "debug": false,
  "scripts": {
    "install": "electron-rebuild",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:makinteractlab/BlinkBoardApp.git"
  },
  "keywords": [
    "BlinkBoard",
    "breadboard",
    "MAKinteract"
  ],
  "author": "Andrea Bianchi",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/icons/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "name": "BlinkBoard",
            "manufacturer": "MAKinteract Lab",
            "ui": {
              "chooseDirectory": true,
              "images": {
                "background": "C:\\Users\\Andrea\\GIT\\BlinkBoardApp\\assets\\images\\installer\\dialog.png",
                "banner": "C:\\Users\\Andrea\\GIT\\BlinkBoardApp\\assets\\images\\installer\\banner.png"
              }
            }
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-wix": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "electron": "^5.0.5",
    "electron-rebuild": "^1.8.5"
  },
  "dependencies": {
    "@octokit/rest": "^18.0.4",
    "chart.js": "^2.9.3",
    "crypto": "^1.0.1",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^7.18.0",
    "jquery": "^3.5.1",
    "serialport": "^7.1.5"
  }
}
